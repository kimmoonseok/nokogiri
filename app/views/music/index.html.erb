<div class="container" style="text-align:center">
    <p>나만의 플레이 리스트</p>
    <% @music.each do |y| %>
    <div class="container">
    <h5 class="mt-3 mb-0"> <%= y.rank %></h5>
    <h5 class="mt-3 mb-0"> <%= y.name %></h5> 
    <img src="<%= y.imge %>"><br> 
        <a href="/music/destroy/<%= y.id %>"> <button type="button" class="btn btn-secondary">삭제</button> </a>
        <a href="/music/edit/<%= y.id %>"> <button type="button" class="btn btn-secondary">수정</button> </a>
        <br>
        </div>
    <% end %>
</div>